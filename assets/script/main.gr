main {
    Camera camera = loadCamera();
    Light light = loadLight(camera);

    Quad quad = loadQuad(camera, light);
    BasicModel model = loadModel(camera, light, "bunny/scene");

    int i;
    loop {
        // Update calls
        camera:update();

        // Draw calls
        light:draw();
        quad:draw();
        model:draw();

        // Next frame
        i++;
        yield
    }
}