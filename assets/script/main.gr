action onLoad() {
    /*Camera camera = Camera();
    setCamera(camera);*/
    let camera = getCamera();
    camera:position(0., .5, 2.);

    Light light = loadLight();

    Quad quad = loadQuad(light);
    BasicModel model = loadModel(light, "bunny/scene");

    integer i;
    loop {
        // Draw calls
        light:draw();
        quad:draw();
        model:draw();

        // Next frame
        i++;
        suspends
    }
}